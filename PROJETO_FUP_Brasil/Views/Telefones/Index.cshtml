@model IEnumerable<Remake_FUP.Models.Telefone>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model._Telefone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item._Telefone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_Telefone">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_Telefone">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_Telefone">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
